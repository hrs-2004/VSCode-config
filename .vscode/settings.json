{
   "files.autoSave": "afterDelay",
   "workbench.preferredLightColorTheme": "Default Dark Modern",
   "editor.semanticHighlighting.enabled": true,
   "workbench.colorCustomizations": {},
   "[html]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
   },
   "[css]": {
      "editor.defaultFormatter": "vscode.css-language-features"
   },
   "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
   },
   "css.lint.unknownAtRules": "ignore",
   "git.autofetch": true,
   "editor.accessibilitySupport": "off",
   "editor.fontFamily": "jetbrains mono",
   "editor.fontWeight": "normal",
   "editor.fontVariations": false,
   "files.autoSaveDelay": 500,
   "terminal.integrated.fontFamily": "JetBrains Mono ",
   "prettier.tabWidth": 2,
   "editor.defaultFormatter": "esbenp.prettier-vscode",
   "notebook.defaultFormatter": "esbenp.prettier-vscode",
   "editor.smoothScrolling": true,
   "window.titleBarStyle": "native",
   "terminal.integrated.gpuAcceleration": "on", 
   "workbench.editor.empty.hint": "hidden",
   "chat.editor.fontFamily": "JetBrains Mono ",
   "editor.codeLensFontFamily": "jetbrains mono",
   "scm.inputFontFamily": "JetBrains Mono ",
   "debug.console.fontFamily": "JetBrains Mono ",
   "editor.inlineSuggest.fontFamily": "JetBrains Mono ",
   "notebook.output.fontFamily": "JetBrains Mono ",
   "debug.console.fontSize": 16,
   "terminal.integrated.fontSize": 15,
   "workbench.navigationControl.enabled": false,
   "editor.fontSize": 15,
   "editor.minimap.sectionHeaderLetterSpacing": 0,
   "tabout.charactersToTabOutFrom": [
      {
         "open": "[",
         "close": "]"
      },
      {
         "open": "{",
         "close": "}"
      },
      {
         "open": "(",
         "close": ")"
      },
      {
         "open": "'",
         "close": "'"
      },
      {
         "open": "\"",
         "close": "\""
      },
      {
         "open": ":",
         "close": ":"
      },
      {
         "open": "=",
         "close": "="
      },
      {
         "open": ">",
         "close": ">"
      },
      {
         "open": "<",
         "close": "<"
      },
      {
         "open": ".",
         "close": "."
      },
      {
         "open": "`",
         "close": "`"
      },
      {
         "open": ";",
         "close": ";"
      }
   ], 
   "editor.guides.bracketPairs": "active",
   "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
   "editor.tabSize": 2,
   "git.openRepositoryInParentFolders": "never",
   "javascript.updateImportsOnFileMove.enabled": "always",
   "workbench.iconTheme": "material-icon-theme",
   "extensions.experimental.affinity": {
      "asvetliakov.vscode-neovim": 1
   },
   "window.menuBarVisibility": "toggle",
   "workbench.activityBar.location": "top",
   "editor.minimap.enabled": false,
   "breadcrumbs.enabled": false,
   "editor.stickyScroll.enabled": false,
   "editor.stickyTabStops": true,
   "editor.inlayHints.fontFamily": "JetBrains Mono ",
   "windsurf.marketplaceExtensionGalleryServiceURL": "https://marketplace.visualstudio.com/_apis/public/gallery",
   "editor.lineNumbers": "relative",
   "editor.lineHeight": 1.5,
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/local/bin/nvim",
   "workbench.tips.enabled": false,
   "workbench.startupEditor": "none",
   "material-icon-theme.hidesExplorerArrows": true,
   "workbench.tree.indent": 10,
   "explorer.compactFolders": false,
   "editor.detectIndentation": false,
   "editor.guides.highlightActiveIndentation": false,
   "editor.guides.indentation": false,
   "editor.occurrencesHighlight": "off",
   "editor.guides.bracketPairsHorizontal": false,
  "editor.glyphMargin": false,
  "window.customTitleBarVisibility": "never"
}
