{
   "files.autoSave": "afterDelay",
   "livePreview.notifyOnOpenLooseFile": false,
   "security.workspace.trust.untrustedFiles": "open",
   "code-runner.runInTerminal": true,
   "liveServer.settings.donotShowInfoMsg": true,
   "editor.wordWrap": "on",
   "editor.renderWhitespace": "none",
   "workbench.preferredLightColorTheme": "Default Dark Modern",
   "editor.semanticHighlighting.enabled": true,
   "workbench.colorCustomizations": {},
   "[html]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
   },
   "[css]": {
      "editor.defaultFormatter": "vscode.css-language-features"
   },
   "[javascript]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
   },
   "css.lint.unknownAtRules": "ignore",
   "git.autofetch": true,
   "liveServer.settings.donotVerifyTags": true,
   "editor.accessibilitySupport": "off",
   "editor.fontFamily": "jetbrains mono",
   "editor.fontWeight": "normal",
   "settingsSync.ignoredSettings": [],
   "editor.fontVariations": false,
   "powermode.shake.enabled": false,
   "powermode.explosions.enabled": false,
   "powermode.combo.counterEnabled": "hide",
   "powermode.combo.timerEnabled": "hide",
   "powermode.presets": "magic",
   "powermode.enabled": true,
   "vim.easymotion": true,
   "vim.cursorStylePerMode.insert": "line",
   "vim.cursorStylePerMode.normal": "block",
   "vim.cursorStylePerMode.visual": "block-outline",
   "liveServer.settings.fullReload": true,
   "files.autoSaveDelay": 500,
   "liveServer.settings.root": "",
   "terminal.integrated.fontFamily": "JetBrains Mono ",
   "cmake.showOptionsMovedNotification": false,
   "prettier.tabWidth": 3,
   "editor.defaultFormatter": "esbenp.prettier-vscode",
   "notebook.defaultFormatter": "esbenp.prettier-vscode",
   "vim.useSystemClipboard": true,
   "editor.smoothScrolling": true,
   "window.titleBarStyle": "native",
   "terminal.integrated.gpuAcceleration": "on",
   "vim.smartRelativeLine": true,
   "vim.handleKeys": {
      "<C-d>": false,
      "<C-s>": false,
      "<C-z>": false,
      "<C-c>": false,
      "<C-v>": false,
      "<C-a>": false,
      "<C-f>": false,
      "<C-w>": false,
      "<C-b>": false,
      "<C-p>": false,
      "<C-n>": false
   },
   "tabnine.experimentalAutoImports": true,
   "workbench.editor.empty.hint": "hidden",
   "chat.editor.fontFamily": "JetBrains Mono ",
   "editor.codeLensFontFamily": "jetbrains mono",
   "scm.inputFontFamily": "JetBrains Mono ",
   "debug.console.fontFamily": "JetBrains Mono ",
   "editor.inlineSuggest.fontFamily": "JetBrains Mono ",
   "notebook.output.fontFamily": "JetBrains Mono ",
   "debug.console.fontSize": 16,
   "terminal.integrated.fontSize": 16,
   "workbench.navigationControl.enabled": false,
   "editor.fontSize": 15,
   "editor.minimap.sectionHeaderLetterSpacing": 0,
   "tabout.charactersToTabOutFrom": [
      {
         "open": "[",
         "close": "]"
      },
      {
         "open": "{",
         "close": "}"
      },
      {
         "open": "(",
         "close": ")"
      },
      {
         "open": "'",
         "close": "'"
      },
      {
         "open": "\"",
         "close": "\""
      },
      {
         "open": ":",
         "close": ":"
      },
      {
         "open": "=",
         "close": "="
      },
      {
         "open": ">",
         "close": ">"
      },
      {
         "open": "<",
         "close": "<"
      },
      {
         "open": ".",
         "close": "."
      },
      {
         "open": "`",
         "close": "`"
      },
      {
         "open": ";",
         "close": ";"
      }
   ],
   "[javascriptreact]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
   },
   "editor.guides.bracketPairs": "active",
   "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
   "editor.tabSize": 3,
   "git.openRepositoryInParentFolders": "never",
   "javascript.updateImportsOnFileMove.enabled": "always",
   "workbench.iconTheme": "material-icon-theme",
   "extensions.experimental.affinity": {
      "asvetliakov.vscode-neovim": 1
   },
   "window.menuBarVisibility": "toggle",
   "workbench.activityBar.location": "top",
   "editor.minimap.enabled": false,
   "breadcrumbs.enabled": false,
   "editor.stickyScroll.enabled": false,
   "editor.stickyTabStops": true,
   "editor.inlayHints.fontFamily": "JetBrains Mono ",
   "windsurf.marketplaceExtensionGalleryServiceURL": "https://marketplace.visualstudio.com/_apis/public/gallery",
   "editor.lineNumbers": "relative",
   "editor.lineHeight": 1.45,
   "window.customTitleBarVisibility": "never",
}
